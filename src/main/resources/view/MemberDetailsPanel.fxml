<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <ImageView fx:id="profilePhoto" fitHeight="152.0" fitWidth="130.0" style="-fx-background-color:white" GridPane.halignment="CENTER">
        <HBox.margin>
            <Insets bottom="7.5" left="7.5" right="5.0" top="7.5" />
        </HBox.margin>
    </ImageView>
   <Label fx:id="name" styleClass="cell_small_label" text="/$name" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
    <FlowPane fx:id="tags" maxWidth="130.0" prefHeight="30.0" prefWidth="109.0" GridPane.halignment="CENTER" GridPane.rowIndex="6" GridPane.valignment="CENTER">
        <padding>
            <Insets bottom="5" top="5" />
        </padding>
    </FlowPane>
    <ImageView fx:id="phoneIcon" fitHeight="21.0" fitWidth="21.0" translateX="10.0" GridPane.halignment="LEFT" GridPane.rowIndex="3">
        <HBox.margin>
            <Insets bottom="7.5" left="7.5" right="5.0" top="7.5" />
        </HBox.margin>
    </ImageView>
   <Label fx:id="group" layoutX="10.0" layoutY="169.0" styleClass="label-group" text="/$group" translateY="-20.0" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM" />
   <Label fx:id="matricNumber" styleClass="cell_small_label" text="/$matricnumber" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
   <Label fx:id="phone" styleClass="cell_small_label" text="/$mobile" GridPane.halignment="CENTER" GridPane.rowIndex="3" />
   <Label fx:id="email" prefWidth="50.0" styleClass="cell_small_label" text="/$email" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
   <ImageView fx:id="emailIcon" fitHeight="21.0" fitWidth="21.0" layoutX="10.0" layoutY="227.0" translateX="10.0" GridPane.rowIndex="4" />
    <rowConstraints>
        <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
    </rowConstraints>
</GridPane>
