<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" prefHeight="76.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">

    <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
        </columnConstraints>
        <VBox alignment="CENTER_LEFT" minHeight="33.0" prefHeight="107.0" prefWidth="150.0" GridPane.columnIndex="0">
            <padding>
                <Insets bottom="5" left="15" right="5" top="5" />
            </padding>
            <HBox alignment="CENTER_LEFT" prefHeight="65.0" prefWidth="135.0" VBox.vgrow="ALWAYS">
                <Label fx:id="choice" styleClass="cell_big_label">
                    <minWidth>
                        <Region fx:constant="USE_COMPUTED_SIZE" />
                    </minWidth>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
                </Label>
                <VBox alignment="CENTER_LEFT">
                    <children>
                        <Label fx:id="answerValue" wrapText="true" />
                    </children>
                </VBox>
            </HBox>
        </VBox>
        <rowConstraints>
            <RowConstraints />
        </rowConstraints>
    </GridPane>
</HBox>
